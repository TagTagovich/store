<form method="post" action="{{ path('user_delete', {'id': user.id}) }}" onsubmit="return confirm('Вы уверены, что хотите удалить этот элемент?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn btn-block btn-lg btn-info btn-rounded">Удалить</button>
</form>
